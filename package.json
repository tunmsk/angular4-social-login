{
  "name": "angular4-social-login",
  "version": "0.2.14",
  "description": "Social login and authentication module for Angular 4",
  "main": "dist/angular4-social-login.min.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "transpile": "ngc",
    "package": "rollup -c",
    "minify": "uglifyjs dist/angular4-social-login.js --screw-ie8 --compress --mangle --comments --output dist/angular4-social-login.min.js",
    "build": "rm -rf dist && npm run transpile && npm run package && npm run minify",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abacritt/angular4-social-login.git"
  },
  "keywords": [
    "angular",
    "typescript",
    "authentication",
    "social",
    "google-signin",
    "facebook-signin",
    "google-login",
    "facebook-login",
    "social-authentication"
  ],
  "dependencies": {
    "@angular/core": "^4.0.0",
    "@angular/common": "^4.0.0",
    "rxjs": "^5.1.0"
  },
  "author": "Jaibatrik Dutta <jaibatrik@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/abacritt/angular4-social-login/issues"
  },
  "homepage": "https://github.com/abacritt/angular4-social-login#readme",
  "devDependencies": {
    "@angular/compiler": "^4.2.6",
    "@angular/compiler-cli": "^4.2.6",
    "@angular/platform-server": "^4.2.6",
    "rollup": "^0.43.0",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "typescript": "^2.4.1",
    "uglify-js": "^3.0.24"
  }
}
